<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>DNS_Simulation</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ae.Dns.AppMetrics" Version="3.0.0" />
    <PackageReference Include="Ae.Dns.Client" Version="3.0.0" />
    <PackageReference Include="Ae.Dns.Protocol" Version="3.0.0" />
    <PackageReference Include="Ae.Dns.Server" Version="3.0.0" />
    <PackageReference Include="Ae.Dns.Server.Http" Version="3.0.0" />
    <PackageReference Include="Microsoft.Extensions.Http.Polly" Version="8.0.3" />
    <PackageReference Include="Polly" Version="8.3.1" />
    <PackageReference Include="System.Data.SQLite" Version="1.0.118" />
  </ItemGroup>

</Project>